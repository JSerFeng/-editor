#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/PROJECT/@editor/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/bin/node_modules:/mnt/d/PROJECT/@editor/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/node_modules:/mnt/d/PROJECT/@editor/node_modules/.pnpm/shelljs@0.8.4/node_modules:/mnt/d/PROJECT/@editor/node_modules/.pnpm/node_modules:/mnt/d/PROJECT/@editor/node_modules:/mnt/d/PROJECT/node_modules:/mnt/d/node_modules:/mnt/d/PROJECT/@editor/packages/cmd/node_modules:/mnt/d/PROJECT/@editor/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/PROJECT/@editor/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/bin/node_modules:/mnt/d/PROJECT/@editor/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/node_modules:/mnt/d/PROJECT/@editor/node_modules/.pnpm/shelljs@0.8.4/node_modules:/mnt/d/PROJECT/@editor/node_modules/.pnpm/node_modules:/mnt/d/PROJECT/@editor/node_modules:/mnt/d/PROJECT/node_modules:/mnt/d/node_modules:/mnt/d/PROJECT/@editor/packages/cmd/node_modules:/mnt/d/PROJECT/@editor/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/bin/shjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/bin/shjs" "$@"
fi
